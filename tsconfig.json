{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "components/*": ["components/*"],
      "utils/*": ["utils/*"],
      "context/*": ["context/*"],
      "types/*": ["types/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}